                <div id="n5"><a href="http://localhost:3000/delete" name="delete">Delete</a></div>
            </section>
        </header>
        <main>
            <h1>All People in Database</h1>

            <span id="disp_grid">
                <ul id="disp_list">
                    <% for(var x=0; x < data.length; x++) { %>
                        <li>
                            <%= data[x].id %>
                        </li>
                    <% } %>
                </ul> 


                <ul id="disp_list">
                    <% for(var x=0; x < data.length; x++) { %>
                        <li>
                            <%= data[x].name %>
                        </li>
                    <% } %>
                </ul> 
                
            </span>


        </main>
    </body>
</html>